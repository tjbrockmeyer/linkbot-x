{
  "name": "linkbot-x",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node dist",
    "dev": "nodemon -r dotenv/config src/index.ts",
    "test": "nyc --check-coverage --reporter none mocha",
    "test:cov": "nyc --no-check-coverage --reporter html mocha && open-cli coverage/index.html",
    "test:ci": "nyc --check-coverage --reporter text-summary mocha --no-colors",
    "build": "rimraf dist && tsc"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "aws-sdk": "^2.1001.0",
    "discord.js": "^13.2.0",
    "mongodb": "^4.1.3",
    "natural": "^5.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/natural": "^5.0.0",
    "@types/node": "^16.10.3",
    "@types/sinon": "^10.0.4",
    "@types/sinon-chai": "^3.2.5",
    "@types/uuid": "^8.3.1",
    "chai": "^4.3.4",
    "dotenv": "^10.0.0",
    "mocha": "^9.1.2",
    "nodemon": "^2.0.13",
    "nyc": "^15.1.0",
    "open-cli": "^7.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.3"
  },
  "mocha": {
    "slow": 40,
    "recursive": true,
    "require": [
      "ts-node/register/transpile-only",
      "source-map-support/register",
      "test/setup.ts"
    ],
    "spec": [
      "test/**/*.ts"
    ],
    "reporter": [
      "dot"
    ]
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "test/**/*.spec.ts",
      "src/index.ts",
      "src/typings/*.ts",
      "src/bot/index.ts",
      "src/bot/sampleMessages.ts",
      "src/database/index.ts"
    ],
    "branches": 90,
    "lines": 90,
    "functions": 90,
    "statements": 90
  }
}
