{
  "name": "linkbot-x",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "dev": "sudo docker compose -f docker/compose.dev.yml up -d && trap 'sudo docker compose -f docker/compose.dev.yml down' EXIT && nodemon -r dotenv/config src/index.ts",
    "test": "nyc --check-coverage --reporter text mocha --reporter spec",
    "test:min": "nyc --check-coverage --reporter text-summary mocha --reporter min",
    "test:cov": "rimraf coverage && nyc --no-check-coverage --reporter html mocha --reporter progress && open-cli coverage/index.html",
    "test:ci": "nyc --check-coverage --reporter text-summary mocha --reporter min --no-colors",
    "clean": "rimraf node_modules dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "discord.js": "^13.2.0",
    "mongodb": "^4.1.3",
    "natural": "^5.1.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@types/chai": "^4.2.22",
    "@types/mocha": "^9.0.0",
    "@types/natural": "^5.0.0",
    "@types/node": "^16.10.3",
    "@types/sinon": "^10.0.4",
    "@types/sinon-chai": "^3.2.5",
    "@types/uuid": "^8.3.1",
    "chai": "^4.3.4",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.13",
    "nyc": "^15.1.0",
    "open-cli": "^7.0.1",
    "rimraf": "^3.0.2",
    "sinon": "^11.1.2",
    "sinon-chai": "^3.7.0",
    "source-map-support": "^0.5.20",
    "ts-node": "^10.2.1",
    "ts-sinon": "^2.0.2",
    "typescript": "^4.4.3"
  },
  "mocha": {
    "slow": 40,
    "recursive": true,
    "require": [
      "ts-node/register/transpile-only"
    ],
    "file": [
      "src/testUtils/setup.ts"
    ],
    "spec": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/testUtils/*"
    ]
  },
  "nyc": {
    "all": true,
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      "src/**/*.spec.ts",
      "src/testUtils/*",
      "src/index.ts",
      "src/typings/*.ts",
      "src/database/**/*.ts",
      "src/bot/data/*.ts",
      "src/bot/index.ts",
      "src/config/*.ts"
    ],
    "branches": 90,
    "lines": 90,
    "functions": 90,
    "statements": 90
  }
}
